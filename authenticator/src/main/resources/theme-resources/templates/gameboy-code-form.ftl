<#import "template.ftl" as layout>
<@layout.registrationLayout displayInfo=true; section>
    <#if section = "title">
        Formulaire code d'accés
    <#elseif section = "header">
        Formulaire code d'accés
    <#elseif section = "form">
        <style type="text/css">
            /*
            * #define J_UP 0x04U
            * #define J_DOWN 0x08U
            * #define J_LEFT 0x02U
            * #define J_RIGHT 0x01U
            * #define J_A 0x10U
            * #define J_B 0x20U
            * #define J_SELECT 0x40U
            * #define J_START 0x80U
            */

        .challenge {
            display:flex;
        }

        .challenge > div {
            height: 4vh;
            background-size: 25vh;
            flex-grow: 1;
        }

        /* Cross Right
        */
        .j1 {
            background-image: url("data:image/svg+xml;base64,")
        }
        /* Cross Left
        */
        .j2 {
            background-image: url("data:image/svg+xml;base64,")
        }
        /* Cross UP
        */
        .j4 {
            background-image: url("data:image/svg+xml;base64,")
        }
        /* Cross Down
        */
        .j8 {
            background-image: url("data:image/svg+xml;base64,")
        }

        /* Button A
        */
        .j10 {
            background-image: url("data:image/svg+xml;base64,")
        }
        /* Button B
        */
        .j20 {
            background-image: url("data:image/svg+xml;base64,")
        }

        </style>
        <div id="kc-form">
            <div id="kc-form-wrapper">
                <p>Veuillez entrer le code d'accés saisir cette combinaison de touches</p>

                <!-- Afficher ici la combinaison de touches -->
                <div class="challenge">
                    <div class="j${challenge0}"></div>
                    <div class="j${challenge1}"></div>
                    <div class="j${challenge2}"></div>
                    <div class="j${challenge3}"></div>
                    <div class="j${challenge4}"></div>
                    <div class="j${challenge5}"></div>
                    <div class="j${challenge6}"></div>
                    <div class="j${challenge7}"></div>
                    <div class="j${challenge8}"></div>
                    <div class="j${challenge9}"></div>
                </div>

                <form action="${url.loginAction}" class="${properties.kcFormClass!}" id="kc-u2f-login-form" method="post">
                    <div class="${properties.kcFormGroupClass!}">
                        
                        <div class="${properties.kcLabelWrapperClass!}">
                            <label for="gameboyCode" class="${properties.kcLabelClass!}">Code d'accés</label>
                        </div>
                        <div class="${properties.kcInputWrapperClass!}">
                            <input id="gameboyCode" tabindex="1" class="${properties.kcInputClass!}" name="gameboyCode" type="text" pattern="[0-9a-f]*"/>
                        </div>
                    </div>

                    <div id="kc-form-buttons" class="${properties.kcFormClass!}">
                        <input tabindex="2" class="gameboy-otp-submit ${properties.kcButtonClass!} ${properties.kcButtonPrimaryClass!} ${properties.kcButtonLargeClass!}"
                            type="submit" value="${msg("doSubmit")}"/>
                    </div>
                </form>
            </div>
        </div>
    </#if>
</@layout.registrationLayout>